<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>ProjetJEE2</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
  	<servlet-name>JSPRecapitulatif</servlet-name>
  	<jsp-file>/WEB-INF/jsp/recapitulatif.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JSPRecapitulatif</servlet-name>
  	<url-pattern>/recapitulatif</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>JSPReservation</servlet-name>
  	<jsp-file>/WEB-INF/jsp/reservation.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JSPReservation</servlet-name>
  	<url-pattern>/reservation</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>JSPPaiement</servlet-name>
  	<jsp-file>/WEB-INF/jsp/paiement.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JSPPaiement</servlet-name>
  	<url-pattern>/paiement</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>JSPOccupation</servlet-name>
  	<jsp-file>/WEB-INF/jsp/occupation.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JSPOccupation</servlet-name>
  	<url-pattern>/occupation</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>JSPCommande</servlet-name>
  	<jsp-file>/WEB-INF/jsp/commande.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
  	<servlet-name>JSPCommande</servlet-name>
  	<url-pattern>/commande</url-pattern>
  </servlet-mapping>
</web-app>